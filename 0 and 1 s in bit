#include <stdio.h>
int main()
{
    int n,c=0,k=0;
    printf("enter the number:");
    scanf("%d",&n);
    while(n>0)
    {
        if(n%2==0)
        {
            n=n/2;
            c++;
            
        }
        else if(n%2!=0)
        {
            n=n/2;
            k++;
        }
    }
    printf("%d\n",c);
    printf("%d\n",k);
    
}
out put:

enter the number:12
2
2


