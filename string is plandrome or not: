#include<stdio.h>
#include<string.h>
#define len strlen(s)
int main()
{
    char s[4];
    printf("enter the string:");
    scanf("%[^\n]s",s);
    int i=0,c=0;
    for(i=0;i<len;i++)
    {
        if(s[i]!='\0)
        {
            c++;
            break;
        }
        
    }
    (c)?printf("not palndrome"):printf("palndrome");

    
}
